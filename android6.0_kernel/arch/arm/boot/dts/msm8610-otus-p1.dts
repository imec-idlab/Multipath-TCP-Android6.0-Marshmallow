/dts-v1/;

/include/ "msm8610-otus-common.dtsi"

/ {
	qcom,msm-id = <147 0x45 0x8100>;
};

&i2c_2 {
	fsa8500@23 {
		status = "ok";
	};
};

&spi_3 {
	stml0xx@0 {
		headset_detect_enable = <0>;
		headset_hw_version = <0>;
	};
};
